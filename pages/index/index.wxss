.container {
  padding: 20rpx;
}

.title {
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
  margin: 40rpx 0;
  color: #333;
}

.table {
  border: 2rpx solid #e5e5e5;
  border-radius: 16rpx;
  overflow: hidden;
  margin: 0 30rpx;
  width: 100%;
}

.table-header {
  background-color: #f8f8f8;
  padding: 28rpx 32rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
  text-align: center;
}

.table-body {
  background-color: #fff;
}

.table-row {
  padding: 28rpx 32rpx;
  font-size: 30rpx;
  color: #666;
  border-top: 2rpx solid #e5e5e5;
  text-align: center;
}

.table-row:first-child {
  border-top: none;
}

.footer {
  text-align: center;
  margin-top: 40rpx;
  font-size: 28rpx;
  color: #999;
}